<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:WpfDiagramDesigner">   
    
    <ControlTemplate TargetType="Control" x:Key="FunctionObjectTemplate">
        <Grid>
            <Ellipse HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Fill="Transparent" Stroke="Black" StrokeThickness="1"/>
        </Grid>
    </ControlTemplate>
    
    <ControlTemplate TargetType="Control" x:Key="TestObjectTemplate">
        <Grid>
            <Path Stroke="Black" StrokeThickness="1" Data="M 0,50 L 50,0 L 100,50 L 50,100 L 0,50"
                  Stretch="Fill" Fill="Transparent"/>
        </Grid>
    </ControlTemplate>
    
</ResourceDictionary>